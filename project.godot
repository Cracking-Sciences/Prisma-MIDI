; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Prisma MIDI"
config/version="0.1"
run/main_scene="res://Scenes/main.tscn"
config/features=PackedStringArray("4.2", "GL Compatibility")
config/icon="res://assets/prisma_midi_prototype.svg"
config/windows_native_icon="res://assets/icon.ico"

[autoload]

Utils="*res://Scripts/utils.gd"

[debug]

gdscript/warnings/integer_division=0

[display]

window/size/viewport_width=1200
window/size/viewport_height=800
window/vsync/vsync_mode=2

[dotnet]

project/assembly_name="Prisma MIDI"

[editor_plugins]

enabled=PackedStringArray("res://addons/midi/plugin.cfg")

[rendering]

renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
